<div class="uk-width-2-3@m  uk-card uk-card-default uk-padding-small uk-visible@m">
  <div class="uk-grid">
    <div class="uk-width-1-6@m">
      <img src="/images/optical.png" width="80" alt="img" />
    </div>
    <div class="uk-width-expand@m uk-heading-small">
      <div class="title">
        {{title}}
      </div>
      <div class="uk-text-muted" style="font-size:xxx-large">
        {{subTitle}}
      </div>
    </div>
    <div class="uk-card uk-card-small uk-text-center uk-padding-remove">
      <a target="_blank" href="https://github.com/dananthonyobrien/Edgeworth-POI-Project/edgeworth">
        <div>
          <i class="fas fa-contribute fa-3x" style="color:rgb(95, 96, 173)" title="Source repo" pos="bottom" uk-tooltip></i>
        </div>
        <div class="uk-width-expand@m uk-text-left">
          <!--<div class="uk-text-muted uk-text-center" style="font-size:xx-small">edgeworth</div>-->
        </div>
      </a>
    </div>
  </div>
</div>
